{
  "manifest_version": 3,
  "name": "Loughborough EventSync",
  "description": "Download Loughborough Calender Events",
  "version": "0.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/hello_icon.png",
      "32": "images/hello_icon.png",
      "48": "images/hello_icon.png",
      "128": "images/hello_icon.png"
    }
  },
  "permissions": ["tabs", "activeTab"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}
